<!DOCTYPE html>
<html lang="en">
<head>
      <% include ../partials/head %>    
</head>

<body class="sticky">
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div><!-- End Preload -->
    
    <!-- wrap Page -->
    <div id="wrap">
        
        <!-- Header Top -->
        <% include ../partials/header-top.ejs%>
        <!-- End Header Top -->
        <!-- Header -->
        <header class="style2">
             <% include ../partials/header %>     
        </header><!-- End Header -->
        <!-- SubHeader -->
        <section class="parallax-window" id="short"  data-parallax="scroll" data-image-src="/public/images/chef.jpg">
            <div id="sub_header">
                <div class="container" id="sub_content">
                    <div class="row">
                        <div class="col-md-12">
                            <h1><%= title %></h1>
                            <div class="bread-crums">
        						<a href="/">Home</a>
        						<span class="bread-crums-span">&raquo;</span>
        						<span class="current"><%= title %></span>
        					</div><!-- End bread-crums -->
                        </div>
                    </div><!-- End row -->
                </div><!-- End container -->
            </div>
        </section>
        <!-- End SubHeader -->
        <div class="gray_bg">
            <div class="container margin_60">
                <% if(data.length > 0) { %>
                <div class="row">
                    <% for(var i=0; i< data.length; i++) { 
                        if(data[i].user_id){
                    %> 
                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                        
                        <div class="col-chef">
                            <div class="photo">
                                <img src="<%= data[i].imageUrl %>" alt="">
                            </div>
                            <div class="chef-overlay">
                               <a href="/dishmizer-profile/<%= data[i].user_id %>"> <h3><%= data[i].value %></h3>
                                <p><%= data[i].content_description %></p>
                                <lable class="star-rating">
                                    <input id="input-2-xs" name="input-2" data-readonly="true" class="rating rating-loading" data-min="0"  data-max="5" data-step="0.5" data-size="xs" value="<%= data[i].rating %>">
                                </lable><!-- End stars -->
                                </a>
                            </div><!-- End chef-overlay -->
                        </div><!-- End col-chef -->
                    </div>
                <% } }%>  
                </div>
                <% } else { %>
                <div class="col-md-12 clearfix">
                        <div class="text-center">
                            <a class="button-more larg-Btn">No Dishmizer Available</a>
                        </div>
                </div>
                <% } %> <!-- End row --> 
                <div class="row">
                    <div class="col-md-12 text-center margin_top_40">
                        <nav class="navigation pagination">
                            <div class="nav-links">
                                <% if (pages > 0) { %>
                                <ul class="pagination text-center">
                                <% if (current == 1) { %>
                                <li class="disabled"><a>First</a></li>
                                <% } else { %>
                                <li><a href="/dishmizer/1">First</a></li>
                                <% } %>
                                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                                <% if (i !== 1) { %>
                                <li class="disabled"><a>...</a></li>
                                <% } %>
                                <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                                <% if (i == current) { %>
                                <li class="active"><a><%= i %></a></li>
                                <% } else { %>
                                <li><a href="/dishmizer/<%= i %>"><%= i %></a></li>
                                <% } %>
                                <% if (i == Number(current) + 4 && i < pages) { %>
                                <li class="disabled"><a>...</a></li>
                                <% } %>
                                <% } %>
                                <% if (current == pages) { %>
                                <li class="disabled"><a>Last</a></li>
                                <% } else { %>
                                <li><a href="/dishmizer/<%= pages %>">Last</a></li>
                                <% } %>
                                </ul>
                                <% } %>

                            </div>
                        </nav>
                    </div>
                </div>
            </div><!-- End container -->
        </div><!-- End white_bg -->
        <!-- newslatter -->
        <div class="newslatter">
            <div class="container">
                <p>Stay Always Updated With us. Sign in with our newsletter</p>
                <div class="subcribe">
                    <input type="text" class="form-control" placeholder="Your Email">
                    <a class="fa fa-paper-plane" href="javascript:void(0);"></a>
                </div>
            </div>
        </div><!-- End newslatter -->
        <!-- Footer -->
        <footer>
            <% include ../partials/footer %>
        </footer><!-- End Footer -->
    </div><!-- End wrap Page -->
    <!-- go-up -->
    <div class="go-up"><i class="fa fa-chevron-up"></i></div>
    <!-- End go-up -->
    
    <!-- COMMON SCRIPTS -->
    <% include ../partials/scripts %>

</body>
</html>